<?xml version="1.0"?>
<PropertyList include="c310p-set.xml">
	<sim>
		<description>Cessna T310P 512 Arrow 325HP (JSBsim)</description>
		<minimum-fg-version>2020.4.0</minimum-fg-version>
		
		<rating>
			<FDM type="int">4</FDM>
			<systems type="int">4</systems>
			<cockpit type="int">4</cockpit>
			<model type="int">5</model>
		</rating>
		
		<flight-model>jsb</flight-model>
		<aero>Systems/t310p-fdm</aero>
		
		<model>
			<path>Aircraft/c310-family/Models/t310p.xml</path>
		</model>
		<variant-of>c310a</variant-of>
		<primary-set type="bool">false</primary-set>
	</sim>

    <limits>
		<mass-and-balance>
			<maximum-ramp-mass-lbs>5700</maximum-ramp-mass-lbs>
			<maximum-takeoff-mass-lbs>5670</maximum-takeoff-mass-lbs>
			<maximum-landing-mass-lbs>5670</maximum-landing-mass-lbs>
			<!--<maximum-zero-fuel-mass-lbs>3475.1</maximum-zero-fuel-mass-lbs>-->
		</mass-and-balance>
	</limits>
	
</PropertyList>
